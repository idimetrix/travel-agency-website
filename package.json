        "material-design-icons-iconfont": "^4.0.3",
        "popper.js": "^1.12"
    },
    "dependencies": {
        "animateme": "^1.1.0",
        "fg-loadcss": "^2.1.0",
        "gsap": "^2.1.1",
        "jquery-bar-rating": "^1.2.2",
        "jquery-lazy": "^1.7.10",
        "jquery-validation": "^1.19.0",
        "js-cookie": "^2.2.0",
        "moment": "^2.23.0",
        "owl.carousel": "^2.3.4",
        "slick-carousel": "^1.8.1",
        "slick-lightbox": "^0.2.10",
        "vue": "^2.5.22",
